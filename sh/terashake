#!/bin/bash -e

[ -x sord ]
mkdir -p tmp/data
./sord
srcdir=$( /bin/pwd )
cd util
./make
cd "$srcdir/tmp"
./tsgrid
mv th x1 x2 x3 meta.m data/
mv nn rlon rlat rdep vm/
if [ $( cat vm/nn ) -gt 4000000 ]; then
  echo "Deferring velocity model generation"
else
  cd vm
  ./scecvm4-s
  mv rho vp vs "$srcdir/tmp/data"
fi

