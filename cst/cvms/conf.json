{
    "file_dep": "dep.bin", 
    "file_lat": "lat.bin", 
    "file_lon": "lon.bin", 
    "file_rho": "rho.bin", 
    "file_vp": "vp.bin", 
    "file_vs": "vs.bin", 
    "host_opts": {
        "alcf_bgp": {
            "launch": "cobalt-mpirun -mode vn -verbose 2 -np {nproc} {command}", 
            "ppn_range": [
                1, 
                2, 
                4
            ]
        }, 
        "alcf_bgq": {
            "launch": "runjob --verbose=INFO --block $COBALT_PARTNAME ${{COBALT_CORNER:+--corner}} $COBALT_CORNER ${{COBALT_SHAPE:+--shape}} $COBALT_SHAPE --envs BG_SHAREDMEMSIZE=32MB PAMID_VERBOSE=1 VPROF_PROFILE=yes -n {nproc} -p {ppn} --exe {command}\\n", 
            "ppn_range": [
                1, 
                2, 
                4, 
                8, 
                16, 
                32
            ]
        }, 
        "nics_kraken": {
            "launch": "aprun -n {nproc} {command}", 
            "ppn_range": []
        }
    }, 
    "iodir": "hold", 
    "max_samples": 4800000, 
    "minutes": 60, 
    "nsample": 0, 
    "nthread": 1, 
    "rundir": "run", 
    "version": "4.0"
}